  "scripts": {
    "clean": "rm -rf lib",
    "transpile": "babel src --watch --presets es2015,stage-0,react --plugins add-module-exports,transform-decorators-legacy --out-dir lib",
    "server-dev": "webpack-dev-server --devtool-eval --hot --inline --colors --progress --display-error-details --content-base build --config config/webpack.dev.config.js",
    "server": "iron-node ./lib/server/server.js",
    "write-to-disk": "webpack --watch --progress --colors --display-error-details -d",
    "deploy": "NODE_ENV=production webpack -p --config config/webpack.prod.config.js",
    "start": "npm run transpile & npm run server-dev & npm run server"
  },
